<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_full"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >


    <!--class="com.salesforce.android.restsample.Tablet.Master.TabletMasterFragmentHome"-->

    <FrameLayout
        android:id="@+id/master_Fragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="40"
        android:name="com.salesforce.android.restsample.Tablet.TabletMasterFragmentHome">
    </FrameLayout>

    <LinearLayout
        style="@style/small_line_vertical_tablet"
        >
    </LinearLayout>

    <FrameLayout
        android:id="@+id/detail_Fragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="60"
        android:name="com.salesforce.android.restsample.Tablet.TabletDetailProgressBar">
    </FrameLayout>


</LinearLayout>